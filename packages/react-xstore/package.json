{
  "name": "@fuels/react-xstore",
  "version": "0.15.3",
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "exports": {
      ".": {
        "require": "dist/index.js",
        "default": "dist/index.mjs"
      }
    },
    "files": [
      "dist"
    ]
  },
  "scripts": {
    "build": "tsup --dts",
    "ts:check": "pnpm xstate:typegen && tsc --noEmit",
    "xstate:typegen": "xstate typegen 'src/**/*.ts?(x)'",
    "test": "jest --verbose",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "@xstate/react": "^3.2.2",
    "xstate": "^4.38.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@fuels/local-storage": "workspace:*",
    "@xstate/inspect": "^0.8.0",
    "@xstate/react": "^3.2.2",
    "fast-equals": "^5.0.1",
    "jotai": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0",
    "xstate": "^4.38.2"
  },
  "devDependencies": {
    "@fuels/jest": "workspace:*",
    "@fuels/ts-config": "workspace:*",
    "@fuels/tsup-config": "workspace:*",
    "@testing-library/react": "^14.0.0",
    "@types/uuid": "^9.0.3",
    "jest-localstorage-mock": "^2.4.26"
  }
}
